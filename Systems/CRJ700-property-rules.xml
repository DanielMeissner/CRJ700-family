<?xml version="1.0" encoding="UTF-8" ?>
<!--
    Property-rules run only at frame rate, while autopilot rules run at FDM rate.
    What is what depends on how the files are included in CRJ700-main.xml
-->
<PropertyList>
    <!-- APU -->
    <logic>
        <name>APU starting</name>
        <input>
            <and>
                <property>controls/APU/off-on</property>
                <greater-than>
                    <property>engines/engine[2]/rpm</property>
                    <value>0</value>
                </greater-than>
                <less-than>
                    <property>engines/engine[2]/rpm</property>
                    <value>46</value>
                </less-than>
            </and>
        </input>
        <output>engines/engine[2]/starting</output>
    </logic>
    <logic>
        <input>
            <and>
                <property>controls/APU/off-on</property>
                <property>engines/engine[2]/running-nasal</property>
            </and>
        </input>
        <output>engines/engine[2]/available</output>
    </logic>
    
    <!-- logic for several EICAS messages -->
    <logic>
        <name>EICAS caution APU gen off</name>
        <input>
            <property>engines/engine[2]/available</property>
            <not>
                <property>controls/electric/engine[2]/generator</property>
            </not>
        </input>
        <output>instrumentation/eicas/caution/apu-gen-off</output>
    </logic>

    <!-- logic for EICAS config warnings -->
    <logic>
        <name>T/O warnings enable</name>
        <input>
            <property>gear/on-ground</property>
            <greater-than>
                <property>engines/engine[0]/n1</property>
                <value>70</value>
            </greater-than>
            <greater-than>
                <property>engines/engine[1]/n1</property>
                <value>70</value>
            </greater-than>
        </input>
        <output>instrumentation/eicas/to-warnings-ena</output>
    </logic>
    <logic>
        <name>EICAS cfg ap</name>
        <input>
            <property>instrumentation/eicas/to-warnings-ena</property>
            <property>controls/autoflight/autopilot/engage</property>
        </input>
        <output>instrumentation/eicas/warning/cfg-ap</output>
    </logic>
    <logic>
        <name>EICAS cfg brakes</name>
        <input>
            <property>instrumentation/eicas/to-warnings-ena</property>
            <property>controls/gear/brake-parking</property>
        </input>
        <output>instrumentation/eicas/warning/cfg-brakes</output>
    </logic>
    <logic>
        <name>EICAS cfg flaps</name>
        <input>
            <property>instrumentation/eicas/to-warnings-ena</property>
            <or>
                <less-than>
                    <property>surface-positions/flap-pos-norm</property>
                    <value>0.177</value>
                </less-than>
                <greater-than>
                    <property>surface-positions/flap-pos-norm</property>
                    <value>0.444</value>
                </greater-than>
            </or>
        </input>
        <output>instrumentation/eicas/warning/cfg-flaps</output>
    </logic>
    <logic>
        <name>EICAS cfg spoilers</name>
        <input>
            <property>instrumentation/eicas/to-warnings-ena</property>
            <or>
                <property>surface-positions/spoiler-ob-ground-pos-norm</property>
                <property>surface-positions/spoiler-ib-ground-pos-norm</property>
                <property>surface-positions/left-ob-mfs-pos-norm</property>
                <property>surface-positions/left-ib-mfs-pos-norm</property>
                <property>surface-positions/right-ib-mfs-pos-norm</property>
                <property>surface-positions/right-ob-mfs-pos-norm</property>
            </or>
        </input>
        <output>instrumentation/eicas/warning/cfg-spoilers</output>
    </logic>
    <!-- config trim -->
    <logic>
        <name>EICAS cfg aileron</name>
        <input>
            <property>instrumentation/eicas/to-warnings-ena</property>
            <greater-than>
                <expression>
                    <abs>
                        <property>controls/flight/aileron-trim</property>
                    </abs>
                </expression>
                <value>0.04</value>
            </greater-than>
        </input>
        <output>instrumentation/eicas/warning/cfg-aileron</output>
    </logic>
    <logic>
        <name>EICAS cfg stab</name>
        <input>
            <property>instrumentation/eicas/to-warnings-ena</property>
            <less-than>
                <property>instrumentation/eicas/hstab-trim</property>
                <value>4</value>
            </less-than>
            <greater-than>
                <property>instrumentation/eicas/hstab-trim</property>
                <value>11</value>
            </greater-than>
        </input>
        <output>instrumentation/eicas/warning/cfg-stab</output>
    </logic>
    <logic>
        <name>EICAS cfg rudder</name>
        <input>
            <property>instrumentation/eicas/to-warnings-ena</property>
            <greater-than>
                <expression>
                    <abs>
                        <property>controls/flight/rudder-trim</property>
                    </abs>
                </expression>
                <value>0.0303</value>
            </greater-than>
        </input>
        <output>instrumentation/eicas/warning/cfg-rudder</output>
    </logic>
    <logic>
        <name>EICAS config trim</name>
        <input>
            <or>
                <property>instrumentation/eicas/warning/cfg-aileron</property>
                <property>instrumentation/eicas/warning/cfg-stab</property>
                <property>instrumentation/eicas/warning/cfg-rudder</property>
            </or>
        </input>
        <output>instrumentation/eicas/warning/cfg-trim</output>
    </logic>
    <logic>
        <name>EICAS advisory T/O ok</name>
        <input>
            <property>instrumentation/eicas/to-warnings-ena</property>
            <not>
                <or>
                    <property>instrumentation/eicas/warning/cfg-ap</property>
                    <property>instrumentation/eicas/warning/cfg-brakes</property>
                    <property>instrumentation/eicas/warning/cfg-flaps</property>
                    <property>instrumentation/eicas/warning/cfg-spoilers</property>
                    <property>instrumentation/eicas/warning/cfg-trim</property>
                </or>
            </not>
        </input>
        <output>instrumentation/eicas/advisory/to-cfg-ok</output>
    </logic>
    <!-- EICAS message inhibits -->
    <logic>
        <name>initial T/O phase enable</name>
        <input>
            <property>gear/on-ground</property>
            <greater-than>
                <property>engines/engine[0]/n1</property>
                <value>79</value>
            </greater-than>
            <greater-than>
                <property>engines/engine[1]/n1</property>
                <value>79</value>
            </greater-than>
            <less-than>
                <property>instrumentation/airspeed-indicator/indicated-speed-kt</property>
                <value>100</value>
            </less-than>
        </input>
        <output>instrumentation/eicas/inhibits/initial-takeoff-set</output>
    </logic>
    <logic>
        <name>initial T/O phase remove</name>
        <input>
            <or>
                <property>instrumentation/eicas/inhibits/final-takeoff-set</property>
                <and>
                    <less-than>
                        <property>engines/engine[0]/n1</property>
                        <value>67.6</value>
                    </less-than>
                    <less-than>
                        <property>engines/engine[1]/n1</property>
                        <value>67.6</value>
                    </less-than>
                </and>
            </or>
        </input>
        <output>instrumentation/eicas/inhibits/initial-takeoff-reset</output>
    </logic>
    <flipflop>
        <type>RS</type> <!-- or SR -->
        <S>
            <property>instrumentation/eicas/inhibits/initial-takeoff-set</property>
        </S>
        <R>
            <property>instrumentation/eicas/inhibits/initial-takeoff-reset</property>
        </R>
        <output>instrumentation/eicas/inhibits/initial-takeoff</output>
    </flipflop>
    <logic>
        <name>final T/O phase enable</name>
        <input>
            <greater-than>
                <property>engines/engine[0]/n1</property>
                <value>79</value>
            </greater-than>
            <greater-than>
                <property>engines/engine[1]/n1</property>
                <value>79</value>
            </greater-than>
            <greater-than>
                <property>instrumentation/airspeed-indicator/indicated-speed-kt</property>
                <value>100</value>
            </greater-than>
        </input>
        <output>instrumentation/eicas/inhibits/final-takeoff-set</output>
    </logic>
    <logic>
        <name>final T/O phase remove</name>
        <input>
            <or>
                <and>
                    <less-than>
                        <property>engines/engine[0]/n1</property>
                        <value>67.6</value>
                    </less-than>
                    <less-than>
                        <property>engines/engine[1]/n1</property>
                        <value>67.6</value>
                    </less-than>
                </and>
                <greater-than>
                    <property>instrumentation/radar-altimeter/radar-altitude-ft</property>
                    <value>400</value>
                </greater-than>
            </or>
        </input>
        <output>instrumentation/eicas/inhibits/final-takeoff-reset</output>
    </logic>
    <flipflop>
        <type>RS</type> <!-- or SR -->
        <S>
            <property>instrumentation/eicas/inhibits/final-takeoff-set</property>
        </S>
        <R>
            <property>instrumentation/eicas/inhibits/final-takeoff-reset</property>
        </R>
        <output>instrumentation/eicas/inhibits/final-takeoff</output>
    </flipflop>

    <logic>
        <name>EICAS inhibits landing phase</name>
        <input>
            <less-than>
                <property>instrumentation/radar-altimeter/radar-altitude-ft</property>
                <value>400</value>
            </less-than>
            <equals>
                <property>gear/gear[0]/position-norm</property>
                <value>1</value>
            </equals>
            <equals>
                <property>gear/gear[1]/position-norm</property>
                <value>1</value>
            </equals>
            <equals>
                <property>gear/gear[2]/position-norm</property>
                <value>1</value>
            </equals>
        </input>
        <output>instrumentation/eicas/inhibits/landing-set</output>
    </logic>
    <filter>
    <!-- for users with throttle quadrant ;) -->
        <name>mixt2speedbrake</name>
        <type>gain</type>
        <debug type="bool">false</debug>
        <gain>1</gain>
        <enable>
            <condition>
                <equals>
                    <property>sim/config/mixt-to-speedbrake</property>
                    <value>1</value>
                </equals>
            </condition>
        </enable>
        <input>
            <expression>
                <dif>
                    <value>1</value>
                    <property>controls/engines/engine/mixture</property>
                </dif>
            </expression>
        </input>
        <output>controls/flight/speedbrake</output>
    </filter>
    <logic>
        <name>hstab powered</name>
        <input>
            <or>
                <greater-than-equals>
                    <property>/systems/AC/outputs/pitch-trim1</property>
                    <value>105</value>
                </greater-than-equals>
                <greater-than-equals>
                    <property>/systems/AC/outputs/pitch-trim2-1</property>
                    <value>105</value>
                </greater-than-equals>
                <greater-than-equals>
                    <property>/systems/AC/outputs/pitch-trim2-2</property>
                    <value>105</value>
                </greater-than-equals>
            </or>
        </input>
        <output>/systems/hstab/powered</output>
    </logic>
    <filter>
        <name>legacy trim</name>
        <type>gain</type>
        <debug type="bool">false</debug>
        <gain>1</gain>
        <input>
            <!-- FG < v2018.1 do not support hstab trim, so we fake it -->
            <condition>
                <greater-than-equals>
                    <property>controls/flight/hstab-trim</property>
                    <value>0.99</value>
                </greater-than-equals>
            </condition>
            <expression>
                <prod>
                    <sum>
                        <property>controls/flight/elevator-trim</property>
                        <value>0.8</value>
                    </sum>
                    <value>-0.1309</value>
                </prod>
            </expression>
        </input>
        <input>
            <property>surface-positions/hstab-yasim-rad</property>
        </input>
        <output>surface-positions/hstab-rad</output>
    </filter>
    <filter>
        <name>eicas-hstab-trim-units</name>
        <type>gain</type>
        <debug type="bool">false</debug>
        <gain>1</gain>
        <!-- map [+2,-13]deg to [15,0]trim units -->
        <input>
            <expression>
                <difference>
                    <product>
                        <property>surface-positions/hstab-rad</property>
                        <value>57.29578</value>
                    </product>
                    <value>17</value>
                </difference>
            </expression>
        </input>
        <output>instrumentation/eicas/hstab-trim</output>
    </filter>
    <filter>
        <name>dhptr</name>
        <type>gain</type>
        <debug type="bool">false</debug>
        <gain>1</gain>
        <input>
            <expression>
                <dif>
                <property>instrumentation/adc/reference/dh</property>
                <property>instrumentation/radar-altimeter/radar-altitude-ft</property>
                </dif>
            </expression>
        </input>
        <output>instrumentation/radar-altimeter/dh-deviation</output>
    </filter>
    <filter>
        <name>mdaptr</name>
        <type>gain</type>
        <debug type="bool">false</debug>
        <gain>1</gain>
        <input>
            <expression>
                <dif>
                <property>instrumentation/altimeter/indicated-altitude-ft</property>
                <property>instrumentation/adc/reference/mda</property>
                </dif>
            </expression>
        </input>
        <output>instrumentation/altimeter/mda-deviation</output>
    </filter>

    <filter>
        <name>alt-ref-deviation</name>
        <type>gain</type>
        <debug type="bool">false</debug>
        <gain>1</gain>
        <input>
            <expression>
                <abs><dif>
                <property>instrumentation/altimeter/setting-inhg</property>
                <property>environment/metar/pressure-inhg</property>
                </dif></abs>
            </expression>
        </input>
        <output>instrumentation/altimeter/ref-deviation</output>
    </filter>
    <filter>
        <name>VS scaler</name>
        <type>gain</type>
        <debug type="bool">false</debug>
        <gain>1</gain>
        <input>
            <property>instrumentation/vertical-speed-indicator/indicated-speed-fpm</property>
            <scale>0.001</scale>
        </input>
        <output>instrumentation/vertical-speed-indicator/indicated-speed-scaled</output>
    </filter>
    <filter>
        <name>SpeedTape</name>
        <type>gain</type>
        <debug type="bool">false</debug>
        <gain>1</gain>
        <input>
            <expression>
                <dif>
                    <property>instrumentation/airspeed-indicator/indicated-speed-kt</property>
                    <value>40</value>
                </dif>
            </expression>
        </input>
        <output>instrumentation/airspeed-indicator/ias-40</output>
    </filter>
    <filter>
        <name>V1marker</name>
        <type>gain</type>
        <debug type="bool">false</debug>
        <gain>1</gain>
        <input>
            <expression>
                <dif>
                    <property>instrumentation/airspeed-indicator/indicated-speed-kt</property>
                    <property>instrumentation/adc/reference/v1</property>
                </dif>
            </expression>
        </input>
        <output>instrumentation/adc/v1-diff</output>
    </filter>
    <filter>
        <name>VRmarker</name>
        <type>gain</type>
        <debug type="bool">false</debug>
        <gain>1</gain>
        <input>
            <expression>
                <dif>
                    <property>instrumentation/airspeed-indicator/indicated-speed-kt</property>
                    <property>instrumentation/adc/reference/vr</property>
                </dif>
            </expression>
        </input>
        <output>instrumentation/adc/vr-diff</output>
    </filter>
    <filter>
        <name>V2marker</name>
        <type>gain</type>
        <debug type="bool">false</debug>
        <gain>1</gain>
        <input>
            <expression>
                <dif>
                    <property>instrumentation/airspeed-indicator/indicated-speed-kt</property>
                    <property>instrumentation/adc/reference/v2</property>
                </dif>
            </expression>
        </input>
        <output>instrumentation/adc/v2-diff</output>
    </filter>
    <filter>
        <name>VTmarker</name>
        <type>gain</type>
        <debug type="bool">false</debug>
        <gain>1</gain>
        <input>
            <expression>
                <dif>
                    <property>instrumentation/airspeed-indicator/indicated-speed-kt</property>
                    <property>instrumentation/adc/reference/vt</property>
                </dif>
            </expression>
        </input>
        <output>instrumentation/adc/vt-diff</output>
    </filter>
    <logic>
        <name>On ground</name>
        <input>
            <or>
                <property>/gear/gear[0]/wow</property>
                <property>/gear/gear[1]/wow</property>
                <property>/gear/gear[2]/wow</property>
            </or>
        </input>
        <output>/gear/on-ground</output>
    </logic>
    <!-- Alarm logic -->
    <logic>
        <name>Overspeed alarm</name>
        <input>
            <or>
                <!-- 3 kts greater than Vmo -->
                <greater-than-equals>
                    <property>instrumentation/airspeed-indicator[0]/indicated-speed-kt</property>
                    <value>338</value>
                </greater-than-equals>
                <!-- 0.01 Mach greater than Mmo -->
                <greater-than-equals>
                    <property>instrumentation/airspeed-indicator[0]/indicated-mach</property>
                    <value>0.86</value>
                </greater-than-equals>
            </or>
        </input>
        <output>sim/alarms/overspeed-alarm</output>
    </logic>

    <filter>
        <name>pfd_pitch</name>
        <type>gain</type>
        <debug type="bool">false</debug>
        <gain>1</gain>
        <input>
            <expression>
                <table>
                <property>/orientation/pitch-deg</property>
                <entry><ind>-90</ind><dep>-1200</dep></entry>
                <entry><ind>-10</ind><dep>-80</dep></entry>
                <entry><ind>-5</ind><dep>-44</dep></entry>
                <entry><ind>0</ind><dep>0</dep></entry>
                <entry><ind>5</ind><dep>44</dep></entry>
                <entry><ind>10</ind><dep>80</dep></entry>
                <entry><ind>90</ind><dep>1200</dep></entry>
                </table>
            </expression>
        </input>
        <output>instrumentation/pfd[0]/pitch</output>
    </filter>


    <filter>
        <name>pfd_vsi_needle</name>
        <type>gain</type>
        <debug type="bool">false</debug>
        <gain>1</gain>
        <input>
            <expression>
                <table>
                <property>/instrumentation/vertical-speed-indicator/indicated-speed-fpm</property>
                <entry><ind>3000</ind><dep>90</dep></entry>
                <entry><ind>2000</ind><dep>77</dep></entry>
                <entry><ind>1000</ind><dep>64.4</dep></entry>
                <entry><ind>0</ind><dep>0</dep></entry>
                <entry><ind>-1000</ind><dep>-64.4</dep></entry>
                <entry><ind>-2000</ind><dep>-77</dep></entry>
                <entry><ind>-3000</ind><dep>-90</dep></entry>
                </table>
            </expression>
        </input>
        <output>instrumentation/pfd[0]/vsi</output>
    </filter>

    <filter>
        <name>pfd_vmo</name>
        <type>gain</type>
        <debug type="bool">false</debug>
        <gain>1</gain>
        <input>
            <condition>
                <and>
                    <equals>
                        <property>/gear/gear[0]/position-norm</property>
                        <value>0</value>
                    </equals>
                    <equals>
                        <property>/gear/gear[1]/position-norm</property>
                        <value>0</value>
                    </equals>
                    <equals>
                        <property>/gear/gear[2]/position-norm</property>
                        <value>0</value>
                    </equals>
                    <equals>
                        <property>/surface-positions/flap-pos-norm</property>
                        <value>0</value>
                    </equals>
                </and>
            </condition>

            <expression>
                <table>
                <property>/instrumentation/altimeter/indicated-altitude-ft</property>
                <entry><ind>0</ind><dep>330</dep></entry>
                <entry><ind>8000</ind><dep>330</dep></entry>
                <entry><ind>8001</ind><dep>335</dep></entry>
                <entry><ind>28300</ind><dep>315</dep></entry>
                </table>
            </expression>
        </input>
        <input>
            <condition>
                <and>
                    <or>
                        <not-equals>
                            <property>/gear/gear[0]/position-norm</property>
                            <value>0</value>
                        </not-equals>
                        <not-equals>
                            <property>/gear/gear[1]/position-norm</property>
                            <value>0</value>
                        </not-equals>
                        <not-equals>
                            <property>/gear/gear[2]/position-norm</property>
                            <value>0</value>
                        </not-equals>
                    </or>
                    <or>
                        <not-equals>
                            <property>/gear/gear[0]/position-norm</property>
                            <value>1</value>
                        </not-equals>
                        <not-equals>
                            <property>/gear/gear[1]/position-norm</property>
                            <value>1</value>
                        </not-equals>
                        <not-equals>
                            <property>/gear/gear[2]/position-norm</property>
                            <value>1</value>
                        </not-equals>
                    </or>
                    <equals>
                        <property>/controls/gear/gear-down</property>
                        <value>0</value>
                    </equals>
                    <equals>
                        <property>/surface-positions/flap-pos-norm</property>
                        <value>0</value>
                    </equals>
                </and>
            </condition>
            <value>200</value>
        </input>
        <input>
            <condition>
                <and>
                    <or>
                        <not-equals>
                            <property>/gear/gear[0]/position-norm</property>
                            <value>0</value>
                        </not-equals>
                        <not-equals>
                            <property>/gear/gear[1]/position-norm</property>
                            <value>0</value>
                        </not-equals>
                        <not-equals>
                            <property>/gear/gear[2]/position-norm</property>
                            <value>0</value>
                        </not-equals>
                    </or>
                    <or>
                        <not-equals>
                            <property>/gear/gear[0]/position-norm</property>
                            <value>1</value>
                        </not-equals>
                        <not-equals>
                            <property>/gear/gear[1]/position-norm</property>
                            <value>1</value>
                        </not-equals>
                        <not-equals>
                            <property>/gear/gear[2]/position-norm</property>
                            <value>1</value>
                        </not-equals>
                    </or>
                    <equals>
                        <property>/controls/gear/gear-down</property>
                        <value>1</value>
                    </equals>
                    <equals>
                        <property>/surface-positions/flap-pos-norm</property>
                        <value>0</value>
                    </equals>
                </and>
            </condition>
            <value>250</value>
        </input>
        <input>
            <condition>
                <and>
                    <and>
                        <equals>
                            <property>/gear/gear[0]/position-norm</property>
                            <value>1</value>
                        </equals>
                        <equals>
                            <property>/gear/gear[1]/position-norm</property>
                            <value>1</value>
                        </equals>
                        <equals>
                            <property>/gear/gear[2]/position-norm</property>
                            <value>1</value>
                        </equals>
                    </and>
                    <equals>
                        <property>/surface-positions/flap-pos-norm</property>
                        <value>0</value>
                    </equals>
                </and>
            </condition>
            <value>250</value>
        </input>
        <input>
            <condition>
                <and>
                    <not>
